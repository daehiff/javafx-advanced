<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>


<BorderPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.openjfx.viewController.ControlElementsController">
    <center>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0">
            <children>
                <Button fx:id="sayItButton" mnemonicParsing="false" text="Button"/>
                <Label fx:id="sayItLabel" text="Label"/>
                <CheckBox mnemonicParsing="false" text="CheckBox"/>
                <ColorPicker/>
                <TextField fx:id="inputTextfield" text="TextField"/>
                <RadioButton mnemonicParsing="false" text="RadioButton"/>
                <Slider/>
                <ChoiceBox prefWidth="150.0"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </center>
    <top>
        <VBox BorderPane.alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="back" onAction="#onBack"/>
            </children>
        </VBox>
    </top>
</BorderPane>
